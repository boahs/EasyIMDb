{
    "name": "Easy IMDb",
    "description": "Parse IMDb ratings, and view the data through a menu.",
    "version": "0.1.2",
    "manifest_version": 2 ,
    "permissions": ["contextMenus"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
}